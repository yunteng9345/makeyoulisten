/* pages/publish/publish.wxss */
.picture_list {
  padding: 20px;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: flex-start;
  align-content: flex-start;
}

.picture_item {
  margin: 10px;
  position: relative;
  width: 160rpx;
  height: 160rpx;
}
.picture_item image {
  width: 160rpx;
  height: 160rpx;
}
.upload{
  height: 100%;border: 1px solid aliceblue;
}
.upload image{
  width: 50px;height: 50px;margin:1rem;
}
